<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>gdg-site</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>
    
    
    
    <static-files>
        <include path="/**.html" />
        <include path="/app/**.js" />
        <include path="/app/scripts/**.js" />
        <include path="/app/scripts/controllers/main.js" />
        <include path="/app/bower_components/angular/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-cookies/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-mocks/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-resource/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-route/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-sanitize/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/angular-scenario/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/bootstrap/dist/js/**.js" expiration="4d 5h" />
        <include path="/app/bower_components/bootstrap/dist/css/**.css" expiration="4d 5h" />
        <include path="/app/bower_components/bootstrap/dist/fonts/**.*" expiration="4d 5h" />
        <include path="/app/bower_components/jquery/**.*" expiration="4d 5h" />
        <include path="/app/bower_components/json3/lib/**.*" expiration="4d 5h" />
        <include path="/app/fonts/**.*" expiration="4d 5h" />
        <include path="/app/images/**.*" expiration="4d 5h" />
        <include path="/app/styles/**.*" expiration="4d 5h" />
        <include path="/app/views/**.*" expiration="4d 5h" />
    </static-files>
    
    <public-root>/app</public-root>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
